generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Animal {
  id            String   @id @default(uuid())
  name          String
  species       String
  binomial_name String
  photo_url     String
  born_date     DateTime

  @@map("animals")
}
